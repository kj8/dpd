{
    "name": "kj8/dpd",
    "description": "DPD",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kj8\\DPD\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "codeigniter4/framework": "^4.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.1",
        "psr/http-message": "^2.0"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Required to use the HTTP client (^7.0).",
        "nyholm/psr7": "A fast PHP 7 implementation of PSR-7 and PSR-17 (^1.0).",
        "symfony/http-client": "To use Symfony's PSR-18 HTTP Client (^6.0|^7.0)."
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.92",
        "overtrue/phplint": "^9.6",
        "phpstan/phpstan": "^2.1",
        "rector/rector": "^2.2"
    },
    "config": {
        "sort-packages": true
    }
}
